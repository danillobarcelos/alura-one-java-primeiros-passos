package sintaxeVariaveisEFluxo;

public class ExercicioFatorial {
	public static void main(String[] args) {
		int soma = 0;
		int fatorial;
		for (int n = 1; n <= 10; n++) {
			if (n * (n - 1) == 0) {
				fatorial = 1;
			} else {
				fatorial = n * (n-1);
			}
			soma = fatorial * fatorial;
		}
		System.out.println(soma);
	}
}
